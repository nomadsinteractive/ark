<?xml version="1.0" encoding="utf-8"?>
<resources>
	<alphabet id="@a001" class="system" text-size="96"/>
	<boolean id="timeout" class="by-timeout" timeout="$t"/>

	<layer id="@l001" class="image-layer">
		<atlas src="alphabet.png">
			<import class="characters" characters="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz1234567890/.(),- "
						  font-width="32" font-height="64"/>
		</atlas>
	</layer>
	<layer id="@l002" class="alphabet-layer" alphabet="@a001" text-color="#ffffffff" />

	<numeric id="linear" class="linear" s="$s" v="$v"/>
	<numeric id="accelerate" class="accelerate" s="$s" v="$v" a="$a"/>
	<numeric id="integral" class="integral" s="$s" v="$v" a="$a"/>
	<numeric id="damper" class="damper" s1="$s1" s2="$s2" v="$v" style="expect: $e"/>
	<numeric id="boundary" class="boundary" delegate="$d" expect="$e"/>
	<numeric id="upper" class="upper" delegate="$d" expect="$e"/>

	<renderer id="fps-counter" class="fps-counter" layer="@l001" message="%.2f / 60"/>
</resources>
